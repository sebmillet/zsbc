define void f(*x, y) {
	x = 12
	print "f:x=", x, ":y=", y, "-"
}

define void h(*r[], p[], q[], n) {
	print "pre:", n, ":r:", r[0], ";", r[1], ":p:", p[0], ";", p[1], ":q:", q[0], ";", q[1], "-"

	if (n >= 6) {
		p[0] = -5
		p[1] = -7
	}

	r[0] = 1000 * p[0] + q[0] + 1
	r[1] = 1000 * p[1] + q[1] + 2
	print "post:", n, ":r:", r[0], ";", r[1], ":p:", p[0], ";", p[1], ":q:", q[0], ";", q[1], "-"
}

h(r[], p[], p[], 1)
print "1:r:", r[0], ";", r[1], ":p:", p[0], ";", p[1], "\n"
h(r[], p[], p[], 2)
print "2:r:", r[0], ";", r[1], ":p:", p[0], ";", p[1], "\n"
h(p[], p[], p[], 3)
print "3:r:", r[0], ";", r[1], ":p:", p[0], ";", p[1], "\n"

p[0] = 5
p[1] = 7
h(p[], p[], p[], 4)
print "4:r:", r[0], ";", r[1], ":p:", p[0], ";", p[1], "\n"

p[0] = 5
p[1] = 7
q[0] = 50
q[1] = 70
h(p[], p[], p[], 5)
print "5:r:", r[0], ";", r[1], ":p:", p[0], ";", p[1], ":q:", q[0], ";", q[1], "\n"

p[0] = 5
p[1] = 7
